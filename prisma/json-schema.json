{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "email": {
          "type": "string"
        },
        "passwordHash": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "parsers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Parser"
          }
        },
        "sessions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        },
        "userRoles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/UserRole"
          }
        }
      },
      "required": ["email", "passwordHash", "updatedAt"]
    },
    "Parser": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "authorId": {
          "type": "integer"
        },
        "site": {
          "type": "string"
        },
        "code": {
          "type": "string"
        },
        "publishedAt": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "author": {
          "$ref": "#/definitions/User"
        }
      },
      "required": ["authorId", "site", "code", "updatedAt"]
    },
    "Role": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": ["string", "null"]
        },
        "userRoles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/UserRole"
          }
        }
      },
      "required": ["name"]
    },
    "UserRole": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "integer"
        },
        "roleId": {
          "type": "integer"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "role": {
          "$ref": "#/definitions/Role"
        }
      },
      "required": ["userId", "roleId"]
    },
    "Session": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "token": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "userId": {
          "type": "integer"
        }
      },
      "required": ["token", "updatedAt", "userId"]
    }
  },
  "type": "object",
  "properties": {
    "user": {
      "$ref": "#/definitions/User"
    },
    "parser": {
      "$ref": "#/definitions/Parser"
    },
    "role": {
      "$ref": "#/definitions/Role"
    },
    "userRole": {
      "$ref": "#/definitions/UserRole"
    },
    "session": {
      "$ref": "#/definitions/Session"
    }
  }
}
