{
  "name": "node-soa",
  "version": "0.0.0",
  "description": "Node js SOA example",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint . && tsc --project .",
    "format": "prettier --list-different **/*.js --write .",
    "start": "node -r dotenv/config src/main.js",
    "build": "prisma generate && node ./prisma/sanitise.js",
    "test": "NODE_ENV=test && node -r dotenv/config && yarn tap --no-coverage",
    "syncEnv": "dotenv-checker --schema .env.schema --env .env",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgolden/node-soa.git"
  },
  "author": "Georg Oldenburger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgolden/node-soa/issues"
  },
  "homepage": "https://github.com/georgolden/node-soa#readme",
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@fastify/auth": "^4.2.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/swagger": "^8.2.1",
    "@fastify/swagger-ui": "^1.3.0",
    "@fastify/websocket": "^7.1.2",
    "@prisma/client": "^4.10.1",
    "config": "^3.3.9",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.4.0",
    "husky": "^8.0.3",
    "pino": "^8.7.0",
    "prisma-json-schema-generator": "^3.1.2",
    "redis": "^4.6.4"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/node": "^18.7.8",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "dotenv-checker": "^1.1.5",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "json-schema-to-ts": "^2.6.2",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.7.1",
    "prisma": "^4.11.0",
    "tap": "^16.3.4",
    "typescript": "^4.7.4"
  }
}
